    "tsup": "^8.0.0",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "langchain": "0.0.197-rc.1",
    "uuid": "^9.0.1"
  }
